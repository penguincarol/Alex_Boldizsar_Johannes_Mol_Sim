<?xml version="1.0" encoding="UTF-8"?>
<Simulation xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:noNamespaceSchemaLocation="../input/XMLFormat.xsd">
    <OutputFile OutputFileName="memTest"/>
    <EndTime>1.0</EndTime>
    <TimeStepSize>0.0005</TimeStepSize>
    <ForceCalculation>
        <LennardJones Epsilon="1.0" Sigma="1.0"/>
        <EnableGrav X="0.0" Y="-0.4" Z="0.0"/>
        <EnableLC>
            <BoundaryBox>
                <BoxSize X="102.0" Y="12.0" Z="102.0"/>
                <Front>Periodic</Front>
                <Rear>Periodic</Rear>
                <Left>Periodic</Left>
                <Right>Periodic</Right>
                <Top>Reflecting</Top>
                <Bottom>Reflecting</Bottom>
            </BoundaryBox>
            <CutoffRadius>2.5</CutoffRadius>
        </EnableLC>
        <EnableMem/>
    </ForceCalculation>
    <AverageBrownianMotion>0.01</AverageBrownianMotion>
    <Dimensions>3</Dimensions>
    <Thermostat>
        <T_Init>40</T_Init>
        <N_Term>1000</N_Term>
    </Thermostat>
    <FileType>
        <Input>
            <ShapeList>
                <Shape>
                    <Membrane Spacing="1.0" Mass="2.0" SpringStrength="300.0" DesiredDistance="0.5">
                        <Position X="1.0" Y="10.0" Z="1.0"/>
                        <Velocity X="0.0" Y="0.0" Z="0.0"/>
                        <Dimensions X="100" Y="1" Z="100"/>
                    </Membrane>
                </Shape>
            </ShapeList>
        </Input>
    </FileType>
</Simulation>